<% if !@list.nil? && !@list.empty? %>
  var htmlStr = "";
  <% @list.each do |user| %>
    htmlStr += "<%= j render partial: "shared/user_bar", locals: { user: user } %>";
  <% end %>
  $("#list_<%="#{@type}"%> .users-list").html(htmlStr);
<% else %>
  <% if @type == "mutual_followers" %>
    $("#list_<%="#{@type}"%> .users-list").html("<i>No mutual followers</i>");
  <% else %>
    $("#list_<%="#{@type}"%> .users-list").html("<i>No <%="#{@type}"%></i>");
  <% end %>
<% end %>
